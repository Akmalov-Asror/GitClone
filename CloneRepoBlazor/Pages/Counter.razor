@page "/counter"
@inject HttpClient Http
@inject CloneRepo.Repositories.RepositoryFetcherJob _repositoryFetcherJob

@code {
    protected override async Task OnInitializedAsync()
    {

        RecurringJob.AddOrUpdate(async () => await _repositoryFetcherJob.FetchRepositoriesJob("GitHubUserName", 10), "*/10 * * * *");
    }
}
